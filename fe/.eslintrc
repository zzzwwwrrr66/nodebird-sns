{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/jsx-filename-extension": 0,
    "no-underscore-dangle":0,
    "react/forbid-prop-types": 0,
    "object-curly-newline": 0,
    "no-nested-ternary": 0
  }
}